#include "main.h"
int _printf(const char *format, ...)
// void print_all(const char * const format, ...)
{	va_list args;
	int i = 0, j;
	char  
	int (*funcs[])(va_list) = {print_char, print_string};
	char *types = "cs";

	va_start(args, format);
	while (format && format[i])
	{ j = 0;
		while (types[j])
	{
		if (format[i] == types[j])
		{
		printf("%s", separation);
		funcs[j](args);
		separation = ", ";
		}
		j++;
	}
	i++;
	}
	printf("\n");
va_end(args);
}